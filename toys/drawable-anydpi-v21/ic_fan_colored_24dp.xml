<vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:height="24dp"
    android:viewportHeight="24.0"
    android:viewportWidth="24.0"
    android:width="24dp">

  <!-- Red upper-left blade. -->
  <path
      android:fillColor="@android:color/holo_red_light"
      android:pathData="M 0,12 c 0,-12 12,-12 12,0"
      />

  <!-- Green upper-right blade. -->
  <path
      android:fillColor="@android:color/holo_green_light"
      android:pathData=" M 12,0 c 12,0 12,12 0,12"/>

  <!-- Blue lower-left blade. -->
  <path
      android:fillColor="@android:color/holo_blue_light"
      android:pathData=" M 12,12 c 0,12 12,12 12,0"/>

  <!-- Orange lower-right blade. -->
  <path
      android:fillColor="@android:color/holo_orange_light"
      android:pathData=" M 12,12 c -12,0 -12,12 0,12"/>

</vector>
